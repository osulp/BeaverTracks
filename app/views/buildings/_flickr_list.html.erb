<% begin %>
<ol>
  <% @photos.each do |p| %>

  <% info = flickr.photos.getInfo(:photo_id => p.id) %>
    <li>
      <%= link_to image_tag(FlickRaw.url_s(info), :class => 'flickr_photo', :title => @building.name + ' - ' + p.title, :border => 0) + p.title, @building.id.to_s + '/' + p.id %>
    </li>
  <% end %>
</ol>

<% rescue Exception %>
  <p>Images are currently unavailable.</p>
<% end %>
